{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env node dist/index.js",
    "dev": "cross-env ts-node src/index.ts",
    "clean": "rm -rf ./frontend/build && rm -rf ./build && rm -rf ./dist",
    "build:fe": "yarn --cwd frontend build && mv ./frontend/build ./build",
    "build:be": "tsc",
    "build": "yarn run build:fe && yarn run build:be",
    "clean:build": "yarn run clean && yarn run build",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "install:deps": "yarn --cwd frontend && yarn install",
    "clean:install:build": "yarn run clean && yarn run install:deps && yarn run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@langchain/community": "^0.0.27",
    "@langchain/core": "^0.1.27",
    "@langchain/openai": "^0.0.14",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "chromadb": "^1.9.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsdom": "^25.0.1",
    "langchain": "^0.1.17",
    "langsmith": "^0.0.41",
    "puppeteer": "^23.4.1"
  }
}
